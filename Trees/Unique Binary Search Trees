class Solution {
public:
    int numTrees(int n) {
        int dp[n+1];
        dp[0]=1;
        dp[1]=1;
        for(int i=2;i<=n;i++)
        { dp[i]=0;
            for(int root=1;root<=i;root++)
            {
                dp[i]+=(dp[i-root]*dp[root-1]);
            }
        }
        return dp[n];
    }
};
